{
  "name": "dealer-app",
  "version": "1.0.0",
  "description": "Toyota Dealer Search & Email Marketing App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"node server.js\" \"npm run dev --prefix client\"",
    "build": "npm install --prefix client && npm run build --prefix client",
    "install:all": "npm install && npm install --prefix client"
  },
  "dependencies": {
    "csv-parse": "^5.5.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
